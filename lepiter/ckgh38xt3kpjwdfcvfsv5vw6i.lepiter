				},
				"paragraphStyle" : {
					"__type" : "textStyle"
				},
				"string" : "### Re-assessment on features"
			},
			{
				"__type" : "textSnippet",
				"children" : {
					"__type" : "snippets",
					"items" : [ ]
				},
				"createEmail" : {
					"__type" : "email",
