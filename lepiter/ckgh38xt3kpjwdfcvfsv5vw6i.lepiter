				"uid" : {
					"__type" : "uid",
					"uidString" : "jppE6y75DQCWl9I+DwtL1A=="
				},
				"paragraphStyle" : {
					"__type" : "textStyle"
				},
				"string" : "Once again the format depends on your tools and framework, but it could look like this:"
			},
			{
				"__type" : "textSnippet",
				"children" : {
					"__type" : "snippets",
					"items" : [ ]
				},
				"createEmail" : {
					"__type" : "email",
					"emailString" : "<unknown>"
				},
				"createTime" : {
					"__type" : "time",
